.panel.panel-default
  .panel-heading
    h3.panel-title User Info
  .panel-body#profileInfoPanelBody
    #profileInfoEmptyStateUser.hidden
      center
        img.img-responsive(src='/img/emptyStates/sadTama.png')
        p.text-muted
          strong This user does not exist.
    #profileInfoPanelBodyContent
      .row
        .col-md-12
          img#profileInfoProfileImg.img-responsive.img-rounded(src='/img/profile_pic.png')
      hr
      .row.social
        .col-md-4.col-xs-4
          a#profileInfoSocialInstagram.btn.btn-default.btn-fab.btn-instagram.hidden(target='blank')
            i.fa.fa-fw.fa-inverse.fa-instagram.material-icons
        .col-md-4.col-xs-4
          a#profileInfoSocialFacebook.btn.btn-default.btn-fab.btn-facebook.hidden(target='blank')
            i.fa.fa-fw.fa-inverse.fa-facebook.material-icons
        .col-md-4.col-xs-4
          a#profileInfoSocialTwitter.btn.btn-default.btn-fab.btn-twitter.hidden(target='blank')
            i.fa.fa-fw.fa-inverse.fa-twitter.material-icons

      br
      dl.dl-horizontal
        dt Name:
        dd#profileInfoUsername
        dt Bio:
        dd#profileInfoBio
